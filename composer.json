{
    "name": "sbk/cron-bundle",
    "description": "With the CronBundle you can configure cron jobs in configuration files",
    "keywords": ["cron", "cronjob", "background", "tasks", "crontab", "symfony2", "bundle"],
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Sebastian Kueck",
            "email": "seb.kueck@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "mtdowling/cron-expression": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "symfony/options-resolver": "~2.1",
        "symfony/process": "~2.1",
        "symfony/console": "~2.1",
        "monolog/monolog": "~1.7.0"
    },
    "autoload": {
        "psr-0": { "Sbk\\Bundle\\CronBundle": "" }
    },
    "target-dir": "Sbk/Bundle/CronBundle"
}
